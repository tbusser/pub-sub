{
    "presets": [
        "@babel/preset-env",
        "@babel/typescript"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties"
    ],

    "env": {
        "es5": {
            "presets": [[
                "@babel/preset-env",
                {
                    "corejs": { "version": 3 },
                    "debug": false,
                    "targets": "> 0.25%, not dead",
                    "useBuiltIns": "usage"
                }
            ]]
        },
        "esm": {
            "presets": [[
                "@babel/preset-env",
                {
                    "targets":
                    {
                        "esmodules": true
                    }
                }
            ]]
        }
    }
}
