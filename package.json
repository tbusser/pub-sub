{
  "name": "@ictoanen/pub-sub",
  "version": "2.0.0",
  "description": "A pub/sub system",
  "jsnext:main": "esm/index.js",
  "main": "index.js",
  "module": "esm/index.js",
  "repository": "https://github.com/tbusser/pub-sub",
  "sideEffects": false,
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "./scripts/build.sh",
