    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "@types/sinon": "^7.5.2",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "mocha": "^7.1.0",
    "mz": "^2.7.0",
    "nyc": "^15.0.0",
    "sinon": "^9.0.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  }
